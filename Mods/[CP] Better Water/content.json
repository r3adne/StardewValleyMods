{
	"Format": "2.0.0",
	"ConfigSchema": {
		"Color": {
		"AllowValues": "Teal, Blue, Bright, Dark",
		"Default": "Teal",
        "Section": "Color and Transparency",
		},
		"Color Island": {
		"AllowValues": "Teal, Blue, Bright, Dark",
		"Default": "Blue",
        "Section": "Color and Transparency",
		},
		"Transparency": {
		"AllowValues": "Opaque, Semi-Opaque, Translucent",
		"Default": "Semi-Opaque",
        "Section": "Color and Transparency",
		"Description": "How clear the water depth is:\nOpaque: The water depth is uniform\nSemi-Opaque: The water depth is darker on deep parts\nTranslucent: The water is clear, but everything underwater is the water's color",
		},
		"Witch Swamp": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Witch Swamp Color": {
		"AllowValues": "Blue, Brown, Green",
		"Default": "Blue",
        "Section": "Extra",
		},
		"More shiny water lines": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"More shiny water lines (Mines)": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"More shiny water lines (Sewers)": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Fill bridge holes": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		"Description": "Some wood bridges has a blue pixels holes, supposed to be water, that is static and doesn't match the water color. This option fills those pixels with an unsaturated brown",
		},
		"Mutant Bug Lair": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Sewers": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Fountain": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		"Description": "Recolors town's fountain water color",
		},
		"Mines": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		},
		"Desert": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		"Description": "Desert water changes color with the current season",
		},
		"Fish pond": {
		"AllowValues": "true, false",
		"Default": "true",
        "Section": "Extra",
		"Description": "Disable it if you got a fish pond mod, its a small tweak to the fish pond water",
		},
		"Beach Farm Sand": {
		"AllowValues": "Better Water, None",
		"Default": "Better Water",
        "Section": "Mod compatibility",
		"Description": "You probably don't need to disable this.\nChanges the Beach Farm's coast sand sprites to be from my mod or none at all.\n* If there is still an animation error, it's not my mod's fault.",
		},
	},
