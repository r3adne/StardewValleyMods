{
	"Format": "1.24.0",

	"ConfigSchema": {
	
		"BuildingsReplaced": {"AllowValues":"Jenkin's Residence, Aurora, Blue Moon, Susan's House, Andy's House, pethouse, Grandpas Shed, Claire's House, Joja Emporium",
					"AllowMultiple": true,
					"AllowBlank": true,
					"Default": "Jenkin's Residence, Aurora, Blue Moon, Susan's House, Andy's House, pethouse, Grandpas Shed, Claire's House, Joja Emporium",
					"Description": "You can anable/disable which buildings retextures you want loaded" },
		"vinesReplaced":{"AllowValues": "true, false",
					"Default": "true",
					"Description": "Enables retexture of vines on clifs" },
		"pethouseoption":{"AllowValues": "1, 2",
					"Default": "1",
					"Description": "Choose what Pethouse design you want" },
		"FallColor":{"AllowValues": "1, 2",
					"Default": "1",
					"Description": "To match recolor mods that have that option -> green(1) orange(2)" },
		"RecolorOverwrite":{"AllowValues": "Vanilla, DNEarthyRecolour, Eemie's, FoliageRedone, Starblue, VibrantPastoral, WittilyNamed",
					"AllowBlank": true,
					"Default": "",
					"Description": "If you want to change automaticcaly matched recolor palette" },
	},
	
	"DynamicTokens": [
		{
		"Name": "ImmersiveFarm",
		"Value": "false",
		},
		{
		"Name": "ImmersiveFarm",
		"Value": "IF2R",
			"When": { "HasMod": "flashshifter.immersivefarm2remastered" },
		},
		{
		"Name": "ImmersiveFarm",
		"Value": "GrandpasFarm",
			"When": { "HasMod": "flashshifter.GrandpasFarm" },
		},
	//checking what recolor is used
		{
		"Name": "recolor",
		"Value": "Vanilla",
		},
		{
		"Name": "recolor",
		"Value": "DNEarthyRecolour",
			"When": { "HasMod": "DaisyNiko.EarthyRecolour" },
		},
		{
		"Name": "recolor",
		"Value": "Eemie's",
			"When": { "HasMod": "minervamaga.CP.eemieMapRecolour" },
		},
		{
		"Name": "recolor",
		"Value": "FoliageRedone",
			"When": { "HasMod": "stardewfoliageredone" },
		},
		{
		"Name": "recolor",
		"Value": "Starblue",
			"When": { "HasMod": "Lita.StarblueValley" },
		},
		{
		"Name": "recolor",
		"Value": "VibrantPastoral",
			"When": { "HasMod": "grapeponta.VibrantPastoralRecolor" },
		},
		{
		"Name": "recolor",
		"Value": "WittilyNamed",
			"When": { "HasMod": "Acerbicon.Recolor" },
		},
	//recolor overwrite
		{
		"Name": "recolor",
		"Value": "{{RecolorOverwrite}}",
			"When": { "RecolorOverwrite |contains=Vanilla, DNEarthyRecolour, Eemie's, FoliageRedone, Starblue, VibrantPastoral, WittilyNamed": true },
		},
	//fall color overwrite
		{
		"Name": "recolor",
		"Value": "{{recolor}}/{{FallColor}}",
			"When": { "recolor |contains=Eemie's, VibrantPastoral": true,
					"Season": "fall" },
		},
	],

